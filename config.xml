<?xml version='1.0' encoding='utf-8'?>
<widget id="com.juno.social" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Juno</name>
    <description>
        esto es una red social comunitaria
    </description>
    <author email="jjvg_12@hotmail.com" href="http://www.google.com">
        Brisleydi
        Yosbely Ramos
        Yoselyn Serrano
        Jose Vaamonde
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" />
    <engine name="android" spec="~6.1.2" />
    <engine name="browser" spec="^5.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.3" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.3" />
    <plugin name="cordova-plugin-safariviewcontroller" spec="^1.4.7" />
</widget>
