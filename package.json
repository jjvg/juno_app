{
  "name": "junoapp",
  "description": "Red Social usando Cordova y vuejs",
  "version": "1.0",
  "author": {
    "name": "Equipo Juno",
    "email": "jjvg_12@hotmail.com",
    "url": "https://github.com/jjvg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjvg/juno_app"
  },
  "bugs": {
    "url": "https://github.com/jjvg/juno_app/issues"
  },
  "homepage": "https://github.com/jjvg/juno_app",
  "license": "MIT",
  "keywords": [
    "ecosystem:phonegap",
    "ecosystem:cordova",
    "cordova:template",
    "phonegap:template",
    "vue.js",
    "webpack",
    "hot-reload"
  ],
  "cordova": {
    "id": "junoapp",
    "platforms": [
      "android",
      "browser",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-safariviewcontroller": {}
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --content-base www",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "android": "cordova run android",
    "ios": "cordova run ios",
    "serve": "cordova serve --port 8080"
  },
  "engine": "node >= 6.0.0",
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "cordova": "^7.0.1",
    "cordova-android": "~6.1.2",
    "cordova-browser": "^5.0.0",
    "cordova-plugin-dialogs": "^1.3.3",
    "cordova-plugin-network-information": "^1.3.3",
    "cordova-plugin-safariviewcontroller": "^1.4.7",
    "cordova-plugin-whitelist": "1",
    "extract-text-webpack-plugin": "^2.1.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "material-design-lite": "^1.3.0",
    "meterialize-css": "^0.100.2",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vue-select": "^2.3.0",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "file-loader": "^0.11.1",
    "url-loader": "^0.6.2",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
